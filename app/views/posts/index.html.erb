<% if user_signed_in? %>
 <%= link_to "LOGOUT", destroy_user_session_path ,method: :delete, data: {confirm: "Are you sure??"}%>
<% end %>
<h1>Welcome to my new blog!!!</h1>
<%= link_to "Create new post", new_post_path %><br><br>
<% @post.each do |post| %>
     <h1><%= post.title %></h1>
     <%= post.content %><br><br>
     <%= link_to "Show", post_path(post) %> |
     <%= link_to "Edit", edit_post_path(post) %> |
     <%= link_to "Delete",post, :method => :delete, :data => {confirm: "Are you sure??"} %><hr>
<% end %><br><br><br>
